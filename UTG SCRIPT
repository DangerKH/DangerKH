	function Shrek()
		Attack = true
		NeutralAnims = false
		local oWS = WalkSpeed
		for i = 0, 2, 0.1 do
			swait()
			local Alpha = .3
			RJ.C0 = RJ.C0:lerp(CFrame.new(-0.0045717326, 0.0650117323, -0.292746365, 0.999985039, -0.0054067187, -0.000930163078, 0.00540099945, 0.940456271, 0.339872152, -0.000962814316, -0.339872032, 0.940471232),Alpha)
