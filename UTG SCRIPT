		for i,v in pairs(models) do
			for _, a in pairs(v:GetChildren()) do
				if v:IsA("Part") and v.Name ~= "Thingy" then
					table.insert(parts, a)
					for i,v in pairs(parts) do
						v.CanCollide = false
					end
				end
			end
		end
	end)
