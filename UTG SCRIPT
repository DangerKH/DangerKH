	function attacktwo()
		Debounces.Slashing = true
		for i = 1, 14 do
			Dmg()
			corw8.C0 = Lerp(corw8.C0, CFrame.new(-0.37, -1, 0) * CFrame.Angles(math.rad(90), math.rad(0), math.rad(-90)), 0.4)
