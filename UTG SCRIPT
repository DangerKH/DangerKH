			if v.Handle:FindFirstChildOfClass("Weld") then v.Handle:FindFirstChildOfClass("Weld"):Destroy() end
		end
	end

	if ANIMATIONHERE then ANIMATIONHERE.Parent = CloneChar end
	-- Created by Nebula_Zorua --
