					removeuseless:AddItem(bigrmagic,.2)
					swait()
				end
			end)()
			g1 = Instance.new("BodyGyro", Root)
			g1.D = 175
			g1.P = 20000
			g1.MaxTorque = Vector3.new(0,9000,0)
			BigFireBall:Play()
			for i = 1, 15 do
				g1.CFrame = g1.CFrame:lerp(CFrame.new(Root.Position,mouse.Hit.p),.2)
