			RH.C0 = RH.C0:lerp(CFrame.new(0.497732252, -1.05967009, -0.03568241, 0.999878228, -0.00509300316, 0.0147562362, -9.85493534e-05, 0.943203092, 0.332217097, -0.0156101063, -0.332178056, 0.943087518)*CF.A(M.R(-20),0,0),Alpha)
