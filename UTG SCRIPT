	newWeld(torso, rleg, 0.5, -1, 0)
	rleg.Weld.C1 = CFrame.new(0, 1, 0)
	newWeld(root, torso, 0, -1, 0)
	torso.Weld.C1 = CFrame.new(0, -1, 0)
